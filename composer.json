{
    "name": "symfony-cmf/search-bundle",
    "type": "symfony-bundle",
    "description": "Symfony SearchBundle",
    "keywords": ["search", "database"],
    "homepage": "http://cmf.symfony.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Symfony CMF Community",
            "homepage": "https://github.com/symfony-cmf/SearchBundle/contributors"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^5.3.9|^7.0",
        "symfony/framework-bundle": "^2.3 || ^3.0",
        "liip/search-bundle": "^1.0 || ^2.0",
        "doctrine/phpcr-odm": "^1.0 || ^2.0",
        "symfony-cmf/routing-bundle": "^1.4"
    },
    "require-dev": {
         "symfony-cmf/testing": "^1.2.6"
    },
    "autoload": {
        "psr-4": {
            "Symfony\\Cmf\\Bundle\\SearchBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.2-dev"
        }
    }
}
